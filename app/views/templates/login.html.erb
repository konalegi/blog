<div class="well">
  <div class="alert alert-danger" ng-show="error">{{error}}</div>

  <div >
    <div class="form-group">
      <label for="exampleInputEmail1">Email address</label>
      <input type="email" class="form-control" id="exampleInputEmail1" placeholder="Enter email" ng-model="email"/>
    </div>

    <div class="form-group">
      <label for="exampleInputPassword1">Password</label>
      <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password" ng-model="password"/>
    </div>
  </div>



  <button  ng-click="login()" class="btn btn-primary" >Login</button>
  <!--<button  ng-click="logout()" class="btn btn-primary" >Logout</button>-->
  <button  ng-click="singup()" class="btn btn-primary" >Register</button>
</div>




